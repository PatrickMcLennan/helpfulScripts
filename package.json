{
	"name": "helpfulScripts",
	"version": "1.0.0",
	"description": "Some helpful scripts that I've written in Python, Bash or Node.",
	"main": "index.js",
	"scripts": {
		"run:allTs": "concurrently 'npm run run:fileNameValidator' 'npm run run:imgScraper' 'npm run run:tsscraper'",
		"tsc:fileNameValidator": "tsc -w './fileNameValidator/fileNameValidator.ts'",
		"lint:fileNameValidator": "tslint -p './tsconfig.json' -e './fileNameValidator/fileNameValidator.ts'",
		"run:fileNameValidator": "concurrently 'npm run tsc:fileNameValidator' 'npm run lint:fileNameValidator'",
		"tsc:imgScraper": "tsc -w './imgScraper/imgScraper.ts'",
		"lint:imgScraper": "tslint -p './tsconfig.json' -e './imgScraper/imgScraper.ts'",
		"run:imgScraper": "concurrently 'npm run tsc:imgScraper' 'npm run lint:imgScraper'",
		"tsc:tsscraper": "tsc -w './tsscraper/tsscraper.ts'",
		"lint:tsscraper": "tslint -p './tsconfig.json' -e './tsscraper/tsscaper.ts'",
		"run:tsscraper": "concurrently 'npm run tsc:tsscraper' 'npm run lint:tsscraper'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PatrickMcLennan/helpfulScripts.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/PatrickMcLennan/helpfulScripts/issues"
	},
	"homepage": "https://github.com/PatrickMcLennan/helpfulScripts#readme",
	"devDependencies": {
		"@types/node": "^12.7.3",
		"@types/puppeteer": "^1.19.1",
		"concurrently": "^4.1.2",
		"eslint": "^6.1.0",
		"eslint-plugin-node": "^9.1.0",
		"prettier-eslint": "^9.0.0",
		"puppeteer": "^1.18.1",
		"tslint": "^5.19.0",
		"typescript": "^3.6.2"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"dotenv": "^8.1.0",
		"googleapis": "^42.0.0"
	}
}
